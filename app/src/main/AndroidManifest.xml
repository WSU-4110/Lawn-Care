<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lawn_care">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".adminViewProperties"></activity>
        <activity android:name=".viewUserList" />
        <activity android:name=".editYourProperty" />
        <activity android:name=".PropertyPage"></activity>
        <activity
            android:name=".workOffered"
            android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity android:name=".viewYourWorkerProfile" />
        <activity android:name=".viewYourProperties" />
        <activity android:name=".addWorkerProfile" />
        <activity android:name=".addProperty" />
        <activity android:name=".SignIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp" />
        <activity android:name=".dash" />
    </application>

</manifest>